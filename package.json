{
  "name": "serenity-bdd",
  "version": "0.0.1",
  "description": "Serenity BDD for JavaScript and TypeScript",
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "homepage": "https://github.com/smartcodeltd/serenity-js",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "npm run clean && npm run unit-tests && npm run verify --silent",
    "clean": "rm -rf ./target",
    "unit-tests": "mocha --opts mocha.opts",
    "verify": "npm run protractor",
    "prepublish": "gulp prepublish",
    "postinstall": "npm run typings -- install",
    "preprotractor": "npm run webdriver:update -- --standalone",
    "protractor": "node ./node_modules/protractor/built/cli.js ./config/protractor.conf.js",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start"
  },
  "dependencies": {
    "cucumber": "1.2.1",
    "cucumber-tsflow": "1.1.0",
    "graceful-fs": "4.1.4",
    "lodash": "4.13.1",
    "mkdirp": "0.5.1",
    "moment": "2.14.1",
    "protractor": "3.3.0",
    "protractor-cucumber-framework": "0.6.0",
    "stack-trace": "0.0.9",
    "util-arity": "1.0.2"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "es7-reflect-metadata": "^1.6.0",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-concat": "2.6.0",
    "gulp-istanbul": "1.0.0",
    "gulp-mocha": "3.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "6.0.2",
    "gulp-typescript": "2.13.6",
    "merge2": "1.0.2",
    "mocha": "3.0.0",
    "mock-fs": "3.11.0",
    "nock": "8.0.0",
    "phantomjs-prebuilt": "2.1.7",
    "remap-istanbul": "0.6.4",
    "run-sequence": "1.2.2",
    "selenium-webdriver": "2.53.3",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "ts-helper": "0.0.1",
    "ts-md5": "^1.2.0",
    "ts-node": "1.2.2",
    "tslint": "3.14.0",
    "typescript": "1.8.10",
    "typings": "1.3.2",
    "commitizen": "2.8.5",
    "coveralls": "2.11.12",
    "cz-conventional-changelog": "1.1.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartcodeltd/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/smartcodeltd/serenity-js/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
