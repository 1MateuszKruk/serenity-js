  "homepage": "http://serenity-js.org",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "keywords": [
    "serenity-js",
    "cucumber",
    "tdd",
    "bdd",
    "test",
