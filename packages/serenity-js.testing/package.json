{
  "name": "serenity-js.testing",
  "private": true,
  "version": "0.0.0",
  "description": "Serenity/JS internal testing tools",
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "homepage": "http://serenity-js.org",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --project=tsconfig.json --config=../../tslint.json --format=prose",
    "test": "exit 0;",
    "package": "tsc --project tsconfig.json",
    "verify": "npm run clean && npm run lint && npm test && npm run package",
    "prepublish": "npm run verify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-molak/serenity-js.git"
  },
  "bugs": {
    "url": "https://github.com/jan-molak/serenity-js/issues"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3"
  },
  "peerDependencies": {
    "serenity-js": "*"
  },
  "devDependencies": {
    "serenity-js": "*",
    "serenity-js.devtools": "*"
  }
}
