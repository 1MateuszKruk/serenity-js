{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": false,
    "noImplicitOverride": false,
    "noImplicitReturns": true,
    "pretty": true,
    "removeComments": false,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "features",
    "spec",
    "node_modules"
  ]
}
