{
  "name": "protractor-cucumber",
  "version": "1.0.0",
  "description": "Integrating Protractor/Cucumber with Serenity.js",
  "main": "index.js",
  "scripts": {
    "test":               "npm run protractor",

    "preprotractor":      "npm run webdriver:update -- --standalone",
    "protractor":         "node ./node_modules/protractor/built/cli.js ./protractor.conf.js",

    "webdriver-manager":  "webdriver-manager",
    "webdriver:update":   "npm run webdriver-manager update",
    "webdriver:start":    "npm run webdriver-manager start",

    "typings":            "typings install",
    "postinstall":        "typings install"
  },
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "license": "Apache-2.0",
  "dependencies": {
    "serenity": "file:../../",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "cucumber": "1.2.1",
    "protractor": "3.3.0",
    "protractor-cucumber-framework": "0.6.0",
    "selenium-webdriver": "2.53.3",
    "ts-node": "0.9.3",
    "typescript": "1.8.10",
    "typings": "1.3.0"
  }
}
