  "license": "Apache-2.0",
  "dependencies": {
    "ts-node": "0.9.3",
    "typescript": "1.8.10",
    "typings": "1.3.0"
  },
  "devDependencies": {
    "serenity": "file:../../",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "cucumber": "1.2.1",
    "protractor": "3.3.0",
    "protractor-cucumber-framework": "0.6.0",
    "selenium-webdriver": "2.53.3",
    "serenity-cli": "^0.1.0"
  }
}
