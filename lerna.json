{
  "lerna": "2.11.0",
  "loglevel": "info",
  "packages": [
    "documentation/*",
    "examples/*",
    "integration/*",
    "packages/*"
  ],
  "version": "2.16.0",
  "command": {
    "create": {
      "homepage": "https://serenity-js.org",
      "license": "Apache-2.0"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "createRelease": "github",
      "exact": true,
      "message": "chore(release): %s\n[ci skip]"
    },
    "publish": {
      "dist-tag": "latest"
    }
  },
  "ignoreChanges": [
    "**/documentation/**",
    "**/examples/**",
    "**/integration/**",
    "**/*.md",
    "**/spec/**",
    "./package.json"
  ]
}
