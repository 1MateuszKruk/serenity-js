{
  "lerna": "2.11.0",
  "loglevel": "silly",
  "packages": [
    "documentation/*",
    "examples/*",
    "integration/*",
    "packages/*"
  ],
  "version": "2.0.1-alpha.0",
  "command": {
    "create": {
      "homepage": "https://serenity-js.org",
      "license": "Apache-2.0"
    },
    "version": {
      "allowBranch": "2.0",
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): %s",
      "preid": "alpha"
    },
    "publish": {
      "dist-tag": "next"
    }
  }
}
