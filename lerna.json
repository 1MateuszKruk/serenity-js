{
  "lerna": "2.11.0",
  "loglevel": "info",
  "packages": [
    "documentation/*",
    "examples/*",
    "integration/*",
    "packages/*"
  ],
  "version": "2.0.8",
  "command": {
    "create": {
      "homepage": "https://serenity-js.org",
      "license": "Apache-2.0"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): %s\n[ci skip]"
    },
    "ignoreChanges": [
      "**/documentation/**",
      "**/examples/**",
      "**/integration/**",
      "**/*.md",
      "**/spec/**"
    ],
    "publish": {
      "dist-tag": "latest"
    }
  }
}
